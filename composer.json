{
    "name": "szymekjanaczek/ebay-sdk-php",
    "description": "An eBay SDK for PHP. Fork of dts/ebay-sdk-php with support for PHP 8.2.",
    "keywords": ["ebay", "api", "sdk", "PHP 8.2"],
    "type": "library",
    "license": "(Apache-2.0 and MIT)",
    "authors": [
        {
            "name": "David T. Sadler",
            "email": "davidtsadler@googlemail.com",
            "homepage": "http://davidtsadler.com"
        },
        {
          "name": "Benjamin Morel",
          "email": "benjamin.morel@gmail.com"
        },
        {
          "name": "Szymon Janaczek",
          "email": "contact@sim-studio.eu",
          "homepage": "https://github.com/szymekjanaczek"
        }
    ],
    "require": {
        "php-64bit": ">=8.0",
        "ext-curl": "*",
        "ext-libxml": "*",
        "ext-xmlwriter": "*",
        "guzzlehttp/guzzle": "^7.5.0",
        "guzzlehttp/psr7": "^2.4.3",
        "guzzlehttp/promises": "^1.5.2"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-tokenizer": "*",
        "ext-simplexml": "*",
        "phpunit/phpunit": "^9.6.3",
        "rector/rector": "^0.15.13",
        "squizlabs/php_codesniffer": "^3.7",
        "phpcsstandards/phpcsextra": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DTS\\eBaySDK\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "DTS\\eBaySDK\\Test\\": "test/"
        },
        "classmap": ["build/"]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
